{
  "name": "md-toc-cli",
  "version": "3.0.1",
  "description": "Automatically insert or update a clickable table of contents (TOC) into your Markdown documents based on its headings using CLI or JavaScript module.",
  "type": "module",
  "bin": "./src/bin/index.js",
  "main": "./src/lib/index.js",
  "scripts": {
    "lint": "eslint .; prettier --check .",
    "format": "prettier --write . && eslint --fix .",
    "test": "mocha",
    "start": "./src/bin/index.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgeniy-khist/markdown-toc.git"
  },
  "keywords": [
    "markdown",
    "toc",
    "table-of-contents",
    "cli"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Eugene Khyst",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/evgeniy-khist/markdown-toc/issues"
  },
  "homepage": "https://github.com/evgeniy-khist/markdown-toc#readme",
  "dependencies": {
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4"
  }
}
